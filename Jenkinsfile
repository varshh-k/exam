pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('build') {
            steps {
                echo 'this is build'
            }
        }
        stage('test') {
            steps {
                echo 'this is test'
            }
        }
        stage('deploy') {
            steps {
                echo 'this is deploy'
            }
        }
        
    }
}
